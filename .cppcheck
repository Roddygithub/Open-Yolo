--enable=warning,performance,portability,style,information,missingInclude
--suppress=missingIncludeSystem
--suppress=unusedFunction
--suppress=unmatchedSuppression
--suppress=unusedStructMember
--suppress=constParameter
--suppress=constParameterCallback
--suppress=useStlAlgorithm
--suppress=redundantAssignment
--inline-suppr
--std=c++17
--platform=unix64
--cppcheck-build-dir=build/cppcheck
--output-file=cppcheck-results.xml
--xml
--xml-version=2
-I ${workspaceFolder}/include
--project=build/compile_commands.json
--suppress=*:*/usr/include/*
--suppress=*:*/usr/local/include/*
--suppress=*:${workspaceFolder}/build/*
--suppress=*:${workspaceFolder}/tests/*
--suppress=*:${workspaceFolder}/third_party/*
--suppress=*:${workspaceFolder}/external/*
--suppress=*:${workspaceFolder}/vendor/*
--suppress=*:${workspaceFolder}/moc_*.cpp
--suppress=*:${workspaceFolder}/*/moc_*.cpp
--suppress=*:${workspaceFolder}/*/qrc_*.cpp
--suppress=*:${workspaceFolder}/*/ui_*.h
--suppress=*:${workspaceFolder}/**/moc_*.cpp
--suppress=*:${workspaceFolder}/**/qrc_*.cpp
--suppress=*:${workspaceFolder}/**/ui_*.h
--suppress=*:${workspaceFolder}/**/*.moc
--suppress=*:${workspaceFolder}/**/*.qrc
--suppress=*:${workspaceFolder}/**/*.ui
--suppress=*:${workspaceFolder}/**/*.qml
--suppress=*:${workspaceFolder}/**/*.js
--suppress=*:${workspaceFolder}/**/*.qdoc
--suppress=*:${workspaceFolder}/**/*.qmltypes
--suppress=*:${workspaceFolder}/**/*.qmlproject
--suppress=*:${workspaceFolder}/**/*.qrc
--suppress=*:${workspaceFolder}/**/*.qss
--suppress=*:${workspaceFolder}/**/*.qtt
--suppress=*:${workspaceFolder}/**/*.qtt
--suppress=*:${workspaceFolder}/**/*.qtt
--suppress=*:${workspaceFolder}/**/*.qtt
